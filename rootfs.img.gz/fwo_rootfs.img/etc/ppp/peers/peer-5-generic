# /etc/ppp/peers/peer-5

# TEMPORARY -- enable debug mode
# debug

# don't require peer to authenticate itself
noauth

# peer doesn't support compression control protocol
noccp

#ask peer for dns servers
usepeerdns

# TEMPORARY -- use of the "-v" option with the chat scripts
connect "/usr/sbin/chat -f /etc/ppp/chat/connect-generic"
disconnect "/usr/sbin/chat -f /etc/ppp/chat/disconnect-generic"

