# connection script for AT&T

TIMEOUT 60

ABORT "BUSY"
ABORT "ERROR"
ABORT "NO ANSWER"
ABORT "NO CARRIER"

SAY "starting connection\n"

# startup
"" "AT"

# TEMPORARY -- display current PLMN information
OK "AT+COPS?"

# define PDP context 1
OK 'AT+CGDCONT=1,"IP","broadband","",0,0'

SAY "dialing...\n"

# dial PDP context 1
OK "ATD*99***1#"

CONNECT "\d\c"

